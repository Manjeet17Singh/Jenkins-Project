pipeline {
	agent any  	
	tools {
	        maven 'apache-maven-3.5.0'
	        jdk 'jdk8'
	}
	stages {  
	  stage ('Compile') {
	      steps {
	                withMaven(maven : 'maven_3_5_0') {
	                    sh 'mvn clean compile'
	                }
	            }
	        }
	     stage ('Testing Stage') {
	            steps {
	                withMaven(maven : 'maven_3_5_0') {
	                    sh 'mvn test'
	                }
	            }
	        }
	  }
}